{
    "name": "imedge/influxdb-streamer",
    "description": "Simple opinionated InfluxDB streamer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\InfluxDbStreamer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\InfluxDbStreamer\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-zlib": "*",
        "amphp/http-client": "^5",
        "amphp/redis": "^2",
        "gipfl/json": ">=0.2",
        "imedge/metrics": ">=0.1",
        "imedge/redis-utils": ">=0.2",
        "psr/log": "^3.0|^1.0",
        "ramsey/uuid": "^4",
        "revolt/event-loop": "^1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
